{
  "name": "jopi-rewrite",
  "version": "2.0.29",
  "publishedDate": "9/29/2025",
  "publicPublishedDate": "9/25/2025",

  "license": "MIT",
  "homepage": "https://github.com/johanpiquet/jopi-rewrite",
  "keywords": [
    "express.js",
    "nextjs",
    "react",
    "react-ssr",
    "react-hydrate"
  ],
  "author": "Johan PIQUET",
  "description": "A web framework for building proxy/cache and react-hydrate applications",
  "engines": {
    "node": ">=22.18.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "@core/dist",
    "@uikit/dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "npm --prefix @core run child:clean && npm --prefix @uikit run child:clean",
    "build": "npm --prefix @core run child:build && npm --prefix @uikit run child:build",
    "bun-publish": "bun publish",
    "untrack": "git rm -r --cached -- ./dist/**;"
  },

  "workspaces": [
    "./@core",
    "./@bundler",
    "./@uikit"
  ],

  "exports": {
    ".": "./@core/dist/index.js",
    "./uikit": "./@uikit/dist/index.js",
    "./bundler": "./@bundler/dist/index.js"
  }
}
